{
    "manifest_version": 3,
    "name" : "UWO Mealplan Calc",
    "description": "Calculates your daily mealplan spendings and your projected remaining balance by the end of the year.",
    "version" : "0.1",
    "permissions": [
        "activeTab",
        "scripting"
    ],  
    "icons": {
        "16": "/images/icon_16.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["analysis.html"],
        "matches": ["<all_urls>"]
      }
    ]
}